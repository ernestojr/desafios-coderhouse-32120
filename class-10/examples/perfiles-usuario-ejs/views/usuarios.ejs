<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>Usuarios</title>
</head>
<body>
  <h1>Usuarios</h1>
  <table id="main">
    <tr>
      <th>ID</th>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Edad</th>
      <th>Email</th>
      <th>Teléfono</th>
    </tr>
    <% if (isEmpty) { %>
      <tr>
        <td>No hay usuarios registrados aún</td>
      </tr>
    <% } %>
    <% usuarios.forEach(function(usuario) { %>
      <%- include('partials/fila-usuario', {usuario: usuario, detailUrlBase: detailUrlBase}); %>
    <% }); %>
  </table>
</body>
</html>